<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/mainBoxMargin"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button6"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="409dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="138dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="409dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="409dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="288dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="513dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:text="Button"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="513dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:layout_marginEnd="12dp"
        android:text="Button"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        tools:layout_editor_absoluteY="288dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="@dimen/buttonWidthHeight"
        android:layout_height="@dimen/buttonWidthHeight"
        android:layout_marginEnd="12dp"
        android:text="Button"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/md_theme_light_primary"
        android:paddingLeft="@dimen/titlePaddingLeftRight"
        android:paddingRight="@dimen/titlePaddingLeftRight"
        android:text="Calculator"
        android:textColor="@color/md_theme_light_onPrimary"
        android:textSize="40sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="15dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>